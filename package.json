{
  "name": "vuejs-embed-websocket",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "manhavn <manhavn@outlook.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "rm -rf dist && node build/build.js",
    "serve-dist": "yarn build && serve -s dist -p 8080",
    "site-deploy": "yarn build && yarn mv-build-gcloud && yarn gcloud-deploy",
    "lint": "eslint --ext .js,.vue src",
    "embed-dev-file": "webpack --config build_embed/webpack.dev.conf.js --progress --hide-modules",
    "embed-build-file": "webpack --config build_embed/webpack.prod.conf.js --progress --hide-modules",
    "start-app-embed": "rm -rf dist && yarn embed-dev-file && webpack-dev-server --inline --progress --config build/webpack.dev-embed.conf.js",
    "build-app-embed": "rm -rf dist && webpack --config build/webpack.prod-embed.conf.js --progress --hide-modules && yarn embed-build-file",
    "serve-dist-embed": "rm -rf local && SERVE=LOCAL yarn build-app-embed && mkdir local && mv dist local && serve -s local -p 8080",
    "mv-build-gcloud": "rm -rf gcloud-deploy/dist && mv dist gcloud-deploy",
    "gcloud-deploy": "gcloud app deploy gcloud-deploy/app.yaml --project moqiz-production",
    "site-embed-deploy": "yarn build-app-embed && yarn mv-build-gcloud && yarn gcloud-deploy",
    "mv-build-firebase": "rm -rf public/dist && mv dist public",
    "firebase-deploy": "yarn build-app-embed && yarn mv-build-firebase && firebase deploy"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "vue": "^2.5.2",
    "vuex": "^3.5.1",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
